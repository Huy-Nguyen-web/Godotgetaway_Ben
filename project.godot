config_version=4

[application]

config/name="GodotGetaway"
run/main_scene="res://Lobby/Lobby.tscn"
config/icon="res://icon.png"
config/windows_native_icon="res://logo1.ico"

[autoload]

Network="*res://Networking/Network.gd"
SaveGame="*res://SaveGame/SaveGame.gd"
Music="*res://Music/Music.tscn"
VolumeControl="*res://Music/VolumeControl.gd"
SceneChanger="*res://GUI/SceneChanger.tscn"
Global="*res://Global.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

